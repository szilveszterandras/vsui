.spinner {
    font-size 5px
    position relative
    text-indent -9999em
    border 1.1em solid rgba($light-blue, 0.2)
    border-left 1.1em solid $light-blue
    transform translateZ(0)
    animation load8 1.1s infinite linear
}
.spinner,
.spinnerafter {
    border-radius 50%
    width 10em
    height 10em
}
@keyframes load8 {
    0% {
        transform rotate(0deg)
    }
    100% {
        transform rotate(360deg)
    }
}
